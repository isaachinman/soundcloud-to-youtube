{
  "extends": "eslint-config-airbnb-base",
  "env": {
    "browser": true
  },
  "rules": {
    "semi": [
      2,
      "never"
    ],
    "padded-blocks": 0,
    "function-paren-newline": 0,
    "no-buffer-constructor": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": 0,
    "import/extensions": 0
  }
}